<h1>Listing note_clients</h1>

<table class="tabla_repeater">
  <tr>
    <th>Client</th>
    <th>Fecha</th>
    <th>Comentario</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @note_clients.each do |note_client| %>
  <tr>
    <td><%= note_client.client %></td>
    <td><%= note_client.fecha %></td>
    <td><%= note_client.comentario %></td>
    <td><%= link_to 'Show', note_client %></td>
    <td><%= link_to 'Edit', edit_note_client_path(note_client) %></td>
    <td><%= link_to 'Destroy', note_client, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

