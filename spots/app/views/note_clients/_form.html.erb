<%= form_for(@note_client) do |f| %>
  <% if @note_client.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@note_client.errors.count, "error") %> prohibited this note_client from being saved:</h2>

      <ul>
      <% @note_client.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<table class="tabla_repeater">
	<tr>
	    <th><%= f.label :client %></th>
	    <td><%= f.collection_select :client_id, Client.all, :id, :nombre, :prompt => true %></td>
	</tr> 	
	<tr>
	    <th><%= f.label :fecha %></th>
	    <td><%= f.date_select :fecha %></td>
	</tr> 	
	<tr>
	    <th><%= f.label :comentario %></th>
	    <td><%= f.text_field :comentario %></td>
	</tr> 
	<tr>		
		<td colspan="2" align="center"><%= f.submit %></td>
	</tr>		
</table>
<% end %>
